#include <stdio.h>
#include <stdlib.h>
#include <time.h>

/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;


	int ar[100];

	n = ar[0];

	int i;

	for (i = 0; ar[i] != '\0'; i++)
	{
		for (i = 0; ar[i] = '\0'; i--)
		{
			printf("Last digit of %d is %d", n, ar[i]);
			if (ar[i] > 5)
			{
				printf(" and is greater than 5\n");
			}
			else if (ar[i] == 0)
			{
				printf(" is zero\n");
			}
			else if ((ar[i] < 6) && (ar[i] != 0))
			{
				printf(" is less than 6 and not 0\n");
			}
		}
	}
	return (0);

}
